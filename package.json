{
  "name": "fp-ts-fluture",
  "version": "0.4.1",
  "description": "fp-ts bindings for Fluture",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "lint": "tslint -p tsconfig.tslint.json src/**/*.ts test/**/*.ts",
    "mocha": "TS_NODE_CACHE=false mocha -r ts-node/register test/*.ts",
    "prettier": "prettier --no-semi --single-quote --print-width 120 --parser typescript --list-different \"{src,test}/**/*.ts\"",
    "test": "npm run prettier && npm run lint && npm run mocha",
    "clean": "rm -rf lib/*",
    "build": "npm run clean && tsc",
    "docs": "docs-ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/fp-ts-fluture.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/fp-ts-fluture/issues"
  },
  "homepage": "https://github.com/gcanti/fp-ts-fluture",
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "2.2.38",
    "@types/node": "8.0.19",
    "docs-ts": "github:gcanti/docs-ts",
    "fluture": "^10.1.0",
    "fp-ts": "^1.14.3",
    "mocha": "3.2.0",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.3",
    "typings-checker": "1.1.2"
  },
  "tags": [
    "fp-ts",
    "fluture",
    "bindings"
  ],
  "keywords": [
    "fp-ts",
    "fluture",
    "bindings"
  ],
  "peerDependencies": {
    "fp-ts": "^1.0.1",
    "fluture": "^10.1.0"
  }
}
