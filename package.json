{
  "name": "fp-ts-fluture",
  "version": "0.6.2",
  "description": "fp-ts bindings for Fluture",
  "files": [
    "lib",
    "es6"
  ],
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -p .",
    "mocha": "TS_NODE_CACHE=false mocha -r ts-node/register test/*.ts",
    "prettier": "prettier --no-semi --single-quote --print-width 120 --parser typescript --list-different \"{src,test}/**/*.ts\"",
    "test": "npm run lint && npm run prettier && npm run mocha && npm run docs",
    "clean": "rm -rf ./lib ./es6",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build-es6.json",
    "postbuild": "import-path-rewrite && prettier --write \"./{lib,es6}/**/*.ts\"",
    "prepublish": "npm run build",
    "docs": "docs-ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/fp-ts-fluture.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/fp-ts-fluture/issues"
  },
  "homepage": "https://github.com/gcanti/fp-ts-fluture",
  "peerDependencies": {
    "fluture": "^12.0.2",
    "fp-ts": "^2.0.0"
  },
  "devDependencies": {
    "@types/mocha": "2.2.38",
    "@types/node": "^12.12.11",
    "docs-ts": "^0.3.0",
    "fluture": "^12.0.2",
    "fp-ts": "^2.0.0",
    "import-path-rewrite": "github:gcanti/import-path-rewrite",
    "mocha": "3.2.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.7.2",
    "typings-checker": "1.1.2"
  },
  "tags": [
    "fp-ts",
    "fluture",
    "bindings"
  ],
  "keywords": [
    "fp-ts",
    "fluture",
    "bindings"
  ]
}
